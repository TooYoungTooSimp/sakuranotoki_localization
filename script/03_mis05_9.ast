		text = {
			vo = {
				{"vo", file="fem_mis_11451", ch="mis"},
			},
		
            cn = {
				{
					"“我身边只有一次真正的艺术家。那个艺术家把所有的痛苦都当成了观察对象，是自然的事情。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「私の手元に、一度だけ本物の芸術家がいた事がある。",
					{"rt2"},
					"その芸術家は、すべての苦痛をまるで観察対象の様に、自然な事として捉えていた」",
					{"rt2"},
				},
