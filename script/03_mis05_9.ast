		text = {
			vo = {
				{"vo", file="fem_mis_11446", ch="mis"},
			},
		
            cn = {
				{
					"“正因为没有把对方视为同等的人，所以用宛如观察对象的动物或昆虫的视线来观察。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「相手を同等の人間として見ていなかったからこそ、あたかも観察対象の動物か昆虫を見るかの様な視線で観察していた」",
					{"rt2"},
				},
