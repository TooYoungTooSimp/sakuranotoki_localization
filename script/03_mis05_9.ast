		text = {
			vo = {
				{"vo", file="fem_mis_11452", ch="mis"},
			},
		
            cn = {
				{
					"“所以我总是很烦躁，给了这位艺术家几拳。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「だから私はいつもイラついて、この芸術家に何度か拳をあげた」",
					{"rt2"},
				},
