		linkback = "block_00037",
		linknext = "block_00039",
		line = 196,
	},
	block_00039 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2a1510", face="a0010", head="mis_z2a", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11437", ch="mis"},
			},
		
            cn = {
				{
					"“我妈妈躲着我。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「母は私を避けていました」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00038",
		linknext = "block_00040",
		line = 203,
	},
	block_00040 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2a1510", face="a0008", head="mis_z2a", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11438", ch="mis"},
			},
		
            cn = {
				{
					"“即使哥哥对我施暴，我也视而不见。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「兄に私が暴力を振るわれても見て見ぬ振り」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00039",
		linknext = "block_00041",
		line = 208,
	},
	block_00041 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2a1510", face="a0001", head="mis_z2a", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11439", ch="mis"},
			},
		
            cn = {
				{
					"“我也是我，对自己、对别人都不关心，所以不怎么在意……”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「私も私で、自分に対しても、他人に対しても無関心だから、あまり気にしませんでした……」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00040",
		linknext = "block_00042",
		line = 215,
	},
	block_00042 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2a1510", face="a0010", head="mis_z2a", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11440", ch="mis"},
			},
		
            cn = {
				{
					"“如果连疼痛都不能赋予它意义的话，那就是空虚了。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「痛みすら、そこに意味が与えられなければ、空虚なものなのです」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00041",
		linknext = "block_00043",
		line = 220,
	},
	block_00043 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2b1500", face="b0001", head="mis_z2b", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11441", ch="mis"},
			},
		
            cn = {
				{
					"“特别是被打的感觉，与其说是疼痛，不如说是事实令人吃惊。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「特に殴られるという感覚は、痛みよりも、その事実にこそ驚きがある」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00042",
		linknext = "block_00044",
		line = 225,
	},
	block_00044 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2b1500", face="b0010", head="mis_z2b", lv=5, id=9},
		{"text"},
		text = {
			vo = {
				{"vo", file="fem_mis_11442", ch="mis"},
			},
		
            cn = {
				{
					"“如果不吃惊的话，被打就不会那么痛苦了。基本上被打后肿起来的时候比较麻烦……”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「驚きがなければ殴られる事はそれほど苦痛ではありません。",
					{"rt2"},
					"だいたい殴られた後に腫れている時の方がめんどくさいぐらいです……」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00043",
		linknext = "block_00045",
		line = 232,
	},
	block_00045 = {
		{"text"},
		text = {
		
            cn = {
				{
					"“被打的时候感觉不到那么痛……这我知道。我小时候也经常被打。”",
				},
			},
        	ja = {
				{
					name = {"直哉"},
					"「殴られた時にそれほど痛みは感じない……それは分かるよ。",
					{"rt2"},
					"俺もガキの頃は良く殴られたから」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00044",
		linknext = "block_00046",
		line = 236,
	},
	block_00046 = {
