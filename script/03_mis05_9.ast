			vo = {
				{"vo", file="fem_mis_11454", ch="mis"},
			},
		
            cn = {
				{
					"“我之所以烦躁，是因为这位艺术家是真的。”",
				},
			},
        	ja = {
				{
					name = {"心鈴"},
					"「私がイラついているのは、この芸術家が本物であるからだ」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00060",
		linknext = "block_00062",
		line = 310,
	},
	block_00062 = {
		{"fg", ch="心鈴", size="z2", mode=3, path=":fg/mis/z2/", file="mis_z2b1500", face="b0010", head="mis_z2b", lv=5, id=9},
